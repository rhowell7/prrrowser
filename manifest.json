{
  "name": "Prrrowser",
  "description": "Browser extension that replaces images with similar images showing cats",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "run_at": "document_start",
      "js": ["content.js"]
    }
  ]
}
